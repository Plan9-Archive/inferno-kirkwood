#!/dis/sh.dis
load std
autoload=(std)

bind -a '#o' /dev	# sdio
bind -a '#Ï†' /dev	# efuse
bind -a '#T' /dev	# sheeva
bind -a '#B' /dev	# boot

echo static | run /netrc

mount {mntgen} /n
memfs /tmp

echo debug off > /dev/sdioctl
echo init > /dev/sdioctl
cat /dev/sdioctl

dossrv  -f /dev/sdiodata -m /n/dos
unmount /n/dos

while {} {
	echo shell...
	sh -i
	echo
}
